#include<stdio.h>
#define max_size 5
int main(){
  int queue[max_size],item;
  int front,rear=-1;
  for(int i=0;i<max_size;i++){
     if(rear==max_size-1){
        printf("No More Elements can be added ...Will be resulting OVERFLOW");
        }
    else{
        rear=rear+1;
        printf("Enter the element to be added:");
        scanf("%d",&item);
        queue[rear]=item;
       }}
  printf("\n The Queue Elements are:\n");
  for(int i=0;i<max_size;i++){
     printf("%d\t",queue[i]);
  }
  int delete_item;
  printf("\nEnter the element from queue to be deleted:");
  scanf("%d",&delete_item);
  for(int i=0;i<max_size;i++){
     if(queue[i]==delete_item){
        continue;
     }
     else{
       printf("\nThe Elements are:%d",queue[i]);
     }
}
return 0;
}
